<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="blocks" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xs:element name="property">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="property" minOccurs="0" maxOccurs="unbounded" />
      </xs:sequence>
      <xs:attribute name="name" type="xs:string" />
      <xs:attribute name="value" type="xs:string" />
      <xs:attribute name="class" type="xs:string" />
      <xs:attribute name="param1" type="xs:string" />
      <xs:attribute name="param2" type="xs:string" />
    </xs:complexType>
  </xs:element>
  <xs:element name="blocks" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="block" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
              <xs:element ref="property"/>
              <xs:element name="drop">
                <xs:complexType>
                  <xs:attribute name="event" type="xs:string" />
                  <xs:attribute name="name" type="xs:string" />
                  <xs:attribute name="count" type="xs:string" />
                  <xs:attribute name="prob" type="xs:decimal" />
                  <xs:attribute name="stick_chance" type="xs:decimal" />
                  <xs:attribute name="tool_category" type="xs:string" />
                </xs:complexType>
              </xs:element>
            </xs:choice>
            <xs:attribute name="id" type="xs:unsignedShort" />
            <xs:attribute name="name" type="xs:string" />
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>